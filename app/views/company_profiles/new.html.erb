<%= form_for :company_profile, url: company_profiles_path do |f| %>
    <p>
        <%= f.label :name %><br/>
        <%= f.text_field :name %>
    </p>
    
    <p>
        <%= f.label :description %><br/>
        <%= f.text_area :description %>
    </p>
    
    <p>
        <%= f.submit %>
    </p>
<% end %>